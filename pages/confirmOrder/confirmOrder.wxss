/* confirmOrder.wxss */
.wrapper-top{
  background: url('../../images/icon/address-icon.png') no-repeat left;
  padding: 15px;
  position: relative;
}
.wrapper-top:after{
  position: absolute;
}
.wrapper-footer {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  border-top:1px solid #e5e5e5;
  height: 50px;
}
.wrapper-footer .count-money{
  text-align: right;
  padding-right: 20px;
}
.wrapper-footer .confirm-btn{
  background-color: #F40;
  color:#fff;
  height: 100%;
  line-height: 50px;
  text-align: center;
  width: 100px; 
}